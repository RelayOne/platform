{
  "name": "@relay/oauth",
  "version": "1.0.0",
  "description": "OAuth 2.0 provider integration for Google, GitHub, and Microsoft",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
