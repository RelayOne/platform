{
  "name": "@relay/rate-limit",
  "version": "1.0.0",
  "description": "Distributed rate limiting with Redis support and in-memory fallback",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "hono": "^4.0.0",
    "ioredis": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "hono": {
      "optional": false
    },
    "ioredis": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^25.0.7",
    "hono": "^4.6.14",
    "ioredis": "^5.4.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.0"
  },
  "keywords": [
    "relay",
    "platform",
    "rate-limiting",
    "rate-limit",
    "redis",
    "middleware",
    "hono"
  ],
  "author": "Terragon Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RelayOne/platform.git",
    "directory": "packages/relay-rate-limit"
  }
}
